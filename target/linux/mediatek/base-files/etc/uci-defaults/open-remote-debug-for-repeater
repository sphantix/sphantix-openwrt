#!/bin/sh
#
# Copyright (C) 2012-2013 hiwifi.com
#
# Open remote debug for repeaters

. /lib/platform.sh

enable_remote_debug() {
	case "$1" in
	HC5642 | HB5801)
		[ ! -e /etc/config/remote_script_enable ] && touch /etc/config/remote_script_enable
		;;
	esac
}

enable_remote_debug $(tw_board_name)

exit 0
